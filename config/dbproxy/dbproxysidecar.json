{
  "containers": [
    {
      "image": "docker.io/infobloxcto/sase.cloud.services:dbproxy-latest",
      "imagePullPolicy": "IfNotPresent",
      "name": "dbproxy",
      "volumeMounts": [
        {
          "mountPath": "/etc/config/dbproxy",
          "name": "postgresdsn",
          "readOnly": true
        }
      ]
    }
  ],
  "volumes": [
    {
      "name": "postgresdsn",
      "secret": {
        "items": [
          {
            "key": "...",
            "path": "db-credential"
          }
        ],
        "optional": false,
        "secretName": "..."
      }
    }
  ]
}
